{
  "name": "lumora-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "worker": "ts-node src/queues/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "seed:demo": "ts-node src/scripts/seedDemo.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.10.1",
    "argon2": "^0.30.0",
    "bullmq": "^1.88.0",
    "ioredis": "^5.3.2",
    "node-clam": "^2.0.1",
    "nodemailer": "^6.9.4",
    "pino": "^8.14.1",
    "@sentry/node": "^7.0.0",
    "prom-client": "^14.0.1",
    "fastify": "^4.22.1",
    "fastify-cors": "^8.4.0",
    "fastify-jwt": "^6.6.0",
    "fastify-multipart": "^6.8.0",
    "fastify-rate-limit": "^6.8.0",
    "fastify-static": "^6.6.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^5.10.1",
    "qrcode": "^1.5.1",
    "sharp": "^0.33.4",
    "tesseract.js": "^4.1.2",
    "uuid": "^9.0.0",
    "zod": "^3.23.2",
    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
